<templateSet group="JavaScript">
  <template name="ld" value="log.data('$FUNCTION$, Line $LINE$ $PARAM_TEXT$: ', $PARAM$);$END$&#10;" description="Insert log.data" toReformat="true" toShortenFQNames="true">
    <variable name="FUNCTION" expression="jsMethodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="LINE" expression="lineNumber()" defaultValue="" alwaysStopAt="false" />
    <variable name="PARAM" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAM_TEXT" expression="" defaultValue="PARAM" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSX_HTML" value="false" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
</templateSet>